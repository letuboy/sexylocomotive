- title t('.title', :title => @photo_gallery.title)

= semantic_form_for @photo, :url => "/admin/photo_galleries/#{@photo_gallery.id}/photos/#{@photo.id}", :html => { :multipart => true } do |form|
  = render 'form', :f => form
  = render 'admin/shared/form_actions', :back_url => admin_photo_gallery_url(@photo_gallery), :button_label => :update

